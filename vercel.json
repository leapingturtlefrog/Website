{
  "redirects": [
    { "source": "/calculator-os", "destination": "/calculator-os/", "permanent": true }
  ],
  "headers": [
    {
      "source": "/calculator-os/",
      "headers": [
        { "key": "Cross-Origin-Opener-Policy", "value": "same-origin" },
        { "key": "Cross-Origin-Embedder-Policy", "value": "require-corp" },
        { "key": "Cross-Origin-Resource-Policy", "value": "cross-origin" }
      ]
    },
    {
      "source": "/calculator-os/(.*)",
      "headers": [
        { "key": "Cross-Origin-Opener-Policy", "value": "same-origin" },
        { "key": "Cross-Origin-Embedder-Policy", "value": "require-corp" },
        { "key": "Cross-Origin-Resource-Policy", "value": "cross-origin" }
      ]
    }
  ],
  "rewrites": [
    { "source": "/calculator-os", "destination": "/calculator-os/web/index.html" },
    { "source": "/calculator-os/", "destination": "/calculator-os/web/index.html" },
    { "source": "/calculator-os/(.*)", "destination": "/calculator-os/web/$1" }
  ]
}